<?xml version="1.0" encoding="UTF-8"?>
<cdmsitemaps>
    <production>

        <!-- web address of your contentDM website (w/o HTTP)-->
        <website>YOURWEBSITEURLHERE</website>
        
        <!-- web address of your contentdm Server (w/o http://) -->
        <server>YOURSERVERURLHERE (probably website plus :81)</server>
        
        <!-- URL path (anything after the base domain) to where Bots can find your sitemaps -->
        <path>sitemap</path>
        
                
        <!-- user configured contentdm web admin -->
        <user>YOURUSERNAMEHERE</user>
        <password>YOURPASSWORDHERE</password>

        <!-- whether to create sitemaps for unpublished collections -->
        <!-- if created, they still aren't referenced in the sitemap index -->
        <unpublished>false</unpublished>

        <!-- log is in the default directory unless placed elsewhere -->
        <!-- if placed elsewhere, you may need to update .htaccess file -->
        <logfile>sitemap.log</logfile>

    </production>
</cdmsitemaps>
